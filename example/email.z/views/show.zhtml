{% extends 'layout.zhtml' %}

{% block contents %}

  {% set encryptedData = contract_get(host, 'Email', 'getByMessageHash', [messageid]) %}
  {% set content = decrypt_one_mail(host, wallet_address, encryptedData) %}

  <div class="formback">
    <b>Message Id</b><br>
    <p>{{ messageid }}</p>
    <b>Message From</b><br>
    <p>{{ content.from }}</p>
    <b>Body</b><br>
    <p>{{ content.message }}</p>
  </div>

{% endblock %}