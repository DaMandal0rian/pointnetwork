{% extends 'layout.zhtml' %}

{% block contents %}

  {% if(is_authenticated(_session_auth)) %}

    {% set contents = storage_get(messageid) %}

    <div class="formback">
      <b>Message Id</b><br>
      <p>{{ messageid }}</p>
      <b>Body</b><br>
      <p>{{ contents }}</p>
    </div>

  {% endif %}

{% endblock %}