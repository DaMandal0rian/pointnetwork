{% extends 'layout.zhtml' %}

{% block contents %}

  {% set emails = retrieve_mail(wallet_address, host, 'Email', 'SendEmail') %}

  <h2>Inbox for {{ wallet_address }}</h2>

  {% for email in emails %}
    <div class="formback">
      <p>
        <b>From</b>:{{ email.from }}
        <a href='/show?messageid={{ email.messageid }}'>{{ email.messageid }}</a>
      </p>
    </div>
  {% endfor %}

{% endblock %}
